<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
	  
	  <link href="/stylesheets/app.css" rel="stylesheet">
	  
	<!-- include font awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
	  
  <link href="https://fonts.googleapis.com/css2?family=Montserrat&family=Permanent+Marker&display=swap" rel="stylesheet">
	  
  <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />

    <title>Shreyas Srivatsa</title>
  </head>
  <body>
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
	  <a class="navbar-brand" href="/">Home</a>
	  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarText" aria-controls="navbarText" 
			  aria-expanded="false" aria-label="Toggle navigation">
		<span class="navbar-toggler-icon"></span>
	  </button>
	  <div class="collapse navbar-collapse" id="navbarText">
		<ul class="navbar-nav ml-auto">
		  <li class="nav-item <%= typeof page !== 'undefined' && page === 'aboutme' ? 'active' : '' %>">
			<a class="nav-link" href="/aboutme">About Me <span class="sr-only">(current)</span></a>
		  </li>
		  <li class="nav-item <%= typeof page !== 'undefined' && page === 'experiences' ? 'active' : '' %>">
			<a class="nav-link" href="/experiences">Experiences</a>
		  </li>
		  <li class="nav-item <%= typeof page !== 'undefined' && page === 'projects' ? 'active' : '' %>">
			<a class="nav-link" href="/projects">Projects</a>
		  </li>
		  <li class="nav-item <%= typeof page !== 'undefined' && page === 'contactme' ? 'active' : '' %>">
			<a class="nav-link" href="/contactme">Contact Me</a>
		  </li>
		</ul>
	  </div>
	</nav>
	  
	<div class="container" style="margin-top:15px;">
		<% if(error && error.length > 0){ %>
			<div class="alert alert-danger" role="alert">
				<%=error%>
			</div>
		<% } %>
		<% if(success && success.length > 0){ %>
			<div class="alert alert-success" role="alert">
				<%=success%>
			</div>
		<% } %>
	</div>  
	  
  <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
  <script>
    AOS.init({disable:false});
  </script>
	  	  
